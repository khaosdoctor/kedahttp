{
	"name": "Go",
	"dockerComposeFile": [
		"../docker-compose.yml"
	],
	"service": "containerscaler",
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined"
	],
	"settings": {
		"terminal.integrated.shell.linux": "/bin/bash",
		"go.gopath": "/go"
	},
	"extensions": [
		"ms-vscode.go"
	],
	"workspaceFolder": "/workspace/",
	"shutdownAction": "none"
}